{
	"name": "sap.fe.demo.travellist",
	"version": "0.0.1",
	"private": true,
	"sapux": true,
	"description": "SAP Fiori elements",
	"keywords": [
		"ui5",
		"openui5",
		"sapui5"
	],
	"main": "webapp/index.html",
	"scripts": {
		"start": "fiori run --open test/flpSandbox.html#Shell-home",
		"start-local": "fiori run --config ./ui5-local.yaml --open test/flpSandbox.html#Shell-home",
		"start-noflp": "fiori run --open 'index.html'",
		"build": "ui5 build -a --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
		"deploy": "fiori verify",
		"deploy-config": "fiori add deploy-config",
		"start-mock": "fiori run --config ./ui5-mock.yaml --open 'index.html#Shell-home'",
		"suite-tests": "fiori run --open 'test/testsuite.qunit.html'",
		"int-tests": "fiori run --open 'test/integration/opaTests.qunit.html'"
	},
	"devDependencies": {
        "@ui5/cli": "^2.11.1",
        "@ui5/fs": "^2.0.6",
        "@ui5/logger": "^2.0.1",
        "@sap/ux-ui5-tooling": "1",
        "rimraf": "3.0.2",
        "@sap/ux-ui5-fe-mockserver-middleware": "1.1.4",
        "@sap/ux-specification": "1"
},
	"ui5": {
		"dependencies": [
			"@sap/ux-ui5-tooling",
			"@sap/ux-ui5-fe-mockserver-middleware"
		]
	}
}
